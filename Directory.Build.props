<Project>
    <PropertyGroup>
      <!--
          <Version>1.0.1.3</Version>
      -->
        <Version>$([System.IO.File]::ReadAllText("$(MSBuildThisFileDirectory)Version.txt"))</Version>
        <Company></Company>
        <Authors>Florian Grimm</Authors>
        <Product>OneStep</Product>
        <Title>OneStep</Title>
        <Copyright>Copyright 2021</Copyright>
        <RootDirectory>$(MSBuildThisFileDirectory)</RootDirectory>
        <BaseIntermediateOutputPath>$(RootDirectory)output\$(MSBuildProjectName)\obj\$(PlatformName)</BaseIntermediateOutputPath>
        <BaseOutputPath>$(RootDirectory)output\$(MSBuildProjectName)\bin\$(PlatformName)</BaseOutputPath>
        <LangVersion>Latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>
</Project>